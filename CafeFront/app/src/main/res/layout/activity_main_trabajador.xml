<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainTrabajador">

    <!-- CAJA -->
    <ImageView
        android:id="@+id/caja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/caja"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <!-- ZONA LIBROS -->
        <!--Harry potter -->
    <TextView
        android:id="@+id/texto_harry"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Harry Potter"
        android:layout_marginTop="65dp"
        app:layout_constraintStart_toStartOf="@id/harry_potter"
        app:layout_constraintEnd_toEndOf="@id/harry_potter"
        app:layout_constraintBottom_toTopOf="@id/harry_potter"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/harry_potter"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/mesa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/texto_harry" />

        <!-- Disney -->
    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Disney"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@id/disney"
        app:layout_constraintEnd_toEndOf="@id/disney"
        app:layout_constraintBottom_toTopOf="@id/disney" />

    <ImageView
        android:id="@+id/disney"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/mesa"
        app:layout_constraintStart_toEndOf="@id/harry_potter"
        app:layout_constraintTop_toTopOf="@id/harry_potter" />

        <!-- Desdentao -->
    <TextView
        android:id="@+id/texto_desdentao"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Desdentao"
        android:textAlignment="center"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="@id/desdentao"
        app:layout_constraintEnd_toEndOf="@id/desdentao"
        app:layout_constraintTop_toBottomOf="@id/harry_potter" />

    <ImageView
        android:id="@+id/desdentao"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        app:layout_constraintStart_toStartOf="@id/harry_potter"
        app:layout_constraintEnd_toEndOf="@id/disney"
        app:layout_constraintTop_toBottomOf="@id/texto_desdentao" />


    <!-- ZONA DEPORTES -->
        <!-- Futbol -->
    <TextView
        android:id="@+id/texto_futbol"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Futbol"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@id/futbol"
        app:layout_constraintEnd_toEndOf="@id/futbol"
        app:layout_constraintBottom_toTopOf="@id/futbol"/>

    <ImageView
        android:id="@+id/futbol"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        android:layout_marginBottom="20dp"
        app:layout_constraintStart_toStartOf="@id/formula"
        app:layout_constraintEnd_toEndOf="@id/tenis"
        app:layout_constraintBottom_toTopOf="@id/texto_formula"/>

        <!--Formula 1 -->
    <TextView
        android:id="@+id/texto_formula"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Formula 1"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@id/formula"
        app:layout_constraintEnd_toEndOf="@id/formula"
        app:layout_constraintBottom_toTopOf="@id/formula"/>

    <ImageView
        android:id="@+id/formula"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        android:layout_marginStart="250dp"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@id/desdentao"
        app:layout_constraintStart_toStartOf="@id/desdentao"/>

        <!-- Tenis -->
    <TextView
        android:id="@+id/texto_tenis"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Tenis"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@id/tenis"
        app:layout_constraintEnd_toEndOf="@id/tenis"
        app:layout_constraintBottom_toTopOf="@id/tenis"/>

    <ImageView
        android:id="@+id/tenis"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        android:layout_marginStart="30dp"
        app:layout_constraintStart_toEndOf="@id/formula"
        app:layout_constraintTop_toTopOf="@id/formula"/>


    <!-- ZONA MÃšSICA -->
        <!-- ECDL -->
    <TextView
        android:id="@+id/texto_ecdl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="El canto"
        android:layout_marginTop="65dp"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/ecdl"
        app:layout_constraintEnd_toEndOf="@id/ecdl"/>

    <ImageView
        android:id="@+id/ecdl"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        android:layout_marginBottom="25dp"
        android:layout_marginStart="75dp"
        app:layout_constraintStart_toEndOf="@id/tenis"
        app:layout_constraintTop_toBottomOf="@id/texto_ecdl"/>


        <!-- Nirvana -->
    <TextView
        android:id="@+id/texto_nirvana"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nirvana"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@id/nirvana"
        app:layout_constraintEnd_toEndOf="@id/nirvana"
        app:layout_constraintBottom_toTopOf="@id/nirvana"/>

    <ImageView
        android:id="@+id/nirvana"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        android:layout_marginStart="30dp"
        app:layout_constraintStart_toEndOf="@id/ecdl"
        app:layout_constraintTop_toTopOf="@id/ecdl" />

        <!-- One direction-->
    <TextView
        android:id="@+id/texto_onedi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="One Direction"
        android:textAlignment="center"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="@id/onedi"
        app:layout_constraintEnd_toEndOf="@id/onedi"
        app:layout_constraintTop_toBottomOf="@id/ecdl" />

    <ImageView
        android:id="@+id/onedi"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:src="@drawable/mesa"
        app:layout_constraintStart_toStartOf="@id/ecdl"
        app:layout_constraintEnd_toEndOf="@id/nirvana"
        app:layout_constraintTop_toBottomOf="@id/texto_onedi" />

</androidx.constraintlayout.widget.ConstraintLayout>